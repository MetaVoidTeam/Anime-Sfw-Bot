{
    "name": "Sfw",
    "description": "Anime Sfw Bot",
    "repository": "https://github.com/MoeZilla/anime-sfw-bot",
    "env": {
        "BOT_TOKEN": {
            "description": "telegram bot token here",
            "required": true
        },
        "API_ID": {
            "description": "telegram api id here",
            "required": true
        },
        "API_HASH": {
            "description": "telegram api hash here",
            "required": true
        },
        "MONGO_URL": {
            "description": "mongo db url",
            "required": true
        },
        "LOG_CHANNEL": {
            "description": "channel id",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
